{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Admin",
        "Client",
        "Magasinier"
      ],
      "default": "Client"
    },
    "nom": {
      "type": "string",
      "required": true,
      "maxLength": 30
    },
    "prenom": {
      "type": "string",
      "maxLength": 30,
      "required": true
    },
    "dateNaissance": {
      "type": "date",
      "required": true
    },
    "adresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::adresse.adresse",
      "mappedBy": "user"
    },
    "commandes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::commande.commande",
      "mappedBy": "user"
    },
    "carte_paiements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::carte-paiement.carte-paiement",
      "mappedBy": "user"
    },
    "commentaires": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::commentaire.commentaire",
      "mappedBy": "user"
    },
    "panier": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::panier.panier",
      "inversedBy": "user"
    }
  }
}
